.pageTop{display:none;}

.page-header .page-header_title{
	margin:0;
}
.page-header .page-header_title .h1_about{
	background: url(/aboutus/img/h1_about01.png) no-repeat center center;
	background-size:330px auto;
}
.about-main_inner{
	width:860px;
	margin:62px auto 142px;
	text-align:center;
	font-size:16px;
	line-height:2.25;
}
p.pageTop {
	position: absolute;
	bottom: 0;
	right: 0;
	z-index: 11;
	background: url(/aboutus/img/arrow_pagetop01.jpg) left top no-repeat;
	background-size: contain;
	width: 40px;
	height: 40px;
	text-indent: -9999px;
}
	p.pageTop a {
		width: 40px;
		height: 40px;
		display: block;
	}
	p.pageTop a:hover {
		opacity: 0.5;
	}
